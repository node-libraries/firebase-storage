{
  "name": "firebase-storage",
  "version": "0.0.2",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "author": "SoraKumo <info@croud.jp>",
  "license": "MIT",
  "description": "Easily operate Google cloud storage with a private key",
  "scripts": {
    "build": "rimraf dist && tsc && tsc -p ./tsconfig.esm.json && cpy esm dist",
    "watch": "tsc -b -w",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "cpy-cli": "^5.0.0",
    "eslint": "8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "firebase",
    "storage"
  ],
  "repository": "https://github.com/node-libraries/firebase-storage"
}
